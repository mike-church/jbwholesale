# Ignore system files and typical project files
*~
.DS_Store
.svn
.cvs
*.bak
*.swp
Thumbs.db
.htaccess

# Sublime Text
*.sublime-workspace

# PHPStorm
*.idea

# Ignore everything in the root except the "wp-content" directory.
/*
!.gitignore
!wp-content/

# ignore everything in the root except the "wp-content" directory.
!wp-content/

# ignore everything in the "wp-content" directory, except:
# "mu-plugins", "plugins", "themes" directory
wp-content/*
!wp-content/mu-plugins/
!wp-content/plugins/
!wp-content/themes/

# Ignore everything in the "plugins" directory, except the plugins you specify
wp-content/plugins/*
!wp-content/plugins/jb-nutritionals/
!wp-content/plugins/jb-feature-video/
!wp-content/plugins/jb-homepage-hero/
!wp-content/plugins/jb-nutritional-facts/
!wp-content/plugins/jb-recipes/
!wp-content/plugins/jb-logos/
!wp-content/plugins/jb-product-notice/
!wp-content/plugins/jb-feature-image/
!wp-content/plugins/woocommerce-local-pickup-email/

# Ignore everything in the "themes" directory, except the themes you specify
wp-content/themes/*
!wp-content/themes/fresh-start/

# ignore node dependency directories
node_modules/

# ignore log files and databases
*.log
*.sql
*.sqlite